<div class="container-fluid">
    <div class="pagination" id="pagination">
        <% if (currentPage > 1) { %>
            <a href="/admin/products?page=<%= currentPage - 1 %>" class="prev">Prev</a>
        <% } %>

        <% if (currentPage > 3) { %>
            <a href="/admin/products?page=1">1</a>
            <% if (currentPage > 4) { %>
                <span>...</span>
            <% } %>
        <% } %>

        <% for (let i = Math.max(1, currentPage - 1); i <= Math.min(totalPages, currentPage + 1); i++) { %>
            <a href="/admin/products?page=<%= i %>" class="<%= currentPage === i ? 'active' : '' %>"><%= i %></a>
        <% } %>

        <% if (currentPage < totalPages - 2) { %>
            <% if (currentPage < totalPages - 3) { %>
                <span>...</span>
            <% } %>
        <% } %>

        <% if (currentPage < totalPages) { %>
            <a href="/admin/products?page=<%= currentPage + 1 %>" class="next">Next</a>
        <% } %>
    </div>
