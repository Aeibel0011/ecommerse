                                        <!-- <form class="d-inline-flex">
                                            <a href="/admin/order" class="align-items-center btn btn-theme d-flex">
                                                <i data-feather="plus"></i>
                                            </a>
                                        </form> -->
                                    </div>

                                    <div class="table-responsive table-product">
                                        <table class="table all-package theme-table" id="table_id">
                                            <thead>
                                                <tr>
                                                    <th> Order Id</th>
                                                    <th>Full Name</th>
                                                 
                                                    <th>Total</th>
                                                    <th>Status</th>
                                                    <th>Date</th>
                                                    
                                                    <th></th>
                                                </tr>
                                            </thead>
                                        
                                            <tbody>

                                            
                                                    <% data.slice().reverse().forEach((datas) => { %>
                                                        <% datas.products.slice().reverse().forEach((product) => { %>
                                                <tr>
                                                    <td><%= datas.orderId %></td>
                                                    <td><%= datas.address.firstName %> <%= datas.address.lastName %></td>
                                                  
                                                    <td>₹<%= product.total + 120 %></td>
                                                    <td><%= product.Status %></td>
                                                    <td><%= datas.date.toLocaleDateString() %></td>
                                                    <td>
                                                        <a id="viewDetailsLink" href="/admin/orderdetails/<%= datas.orderId %>?productid=<%= product.productId._id %>" class="btn btn-info btn-sm">View Details</a>
                                                      </td>
                                                </tr>
                                                <% }) %>
                                                <% }) %>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <script>
                    // Assuming product.Status holds the status value
                    if (product.Status === 'cancelled') {
                      // Disable the link
                      document.getElementById('viewDetailsLink').setAttribute('disabled', 'disabled');
                      // Optionally, change its appearance to indicate it's disabled
                      document.getElementById('viewDetailsLink').classList.add('disabled');
                    }
                  </script>
                <!-- All User Table Ends-->
                            <!-- Pagination -->
    <div class="pagination">
        <% if (currentPage > 1) { %>
